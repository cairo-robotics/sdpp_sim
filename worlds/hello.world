<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <model name="person">
      <pose>0 0 0.5  0 0 0</pose>
      <include>
        <static>false</static>
        <uri>model://person_standing</uri>
      </include>
      <!-- reference to your plugin -->
      <plugin name="model_walk" filename="libmodel_walk.so"/>
    </model>
  </world>
</sdf>